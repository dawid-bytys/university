generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id           String   @id @default(uuid())
  title        String
  description  String
  tags         String[]
  requirements String[]
  benefits     String[]
  image        String
  salary       Int
  company      String
  location     String
  createdAt    DateTime @default(now())
}
